{
    "name": "bot-or-not-backend",
    "version": "1.0.0",
    "description": "Game to test who the bot is",
    "main": "index.ts",
    "scripts": {
        "dev": "PORT=3000 nodemon index.ts",
        "prebuild": "rimraf ./dist",
        "build": "tsc && cd client && npm run build",
        "postbuild": "cp -r views ./dist/views",
        "generate": "prisma generate",
        "migrate-dev": "prisma migrate dev",
        "migrate-deploy": "prisma migrate deploy",
        "migrate-reset": "prisma migrate reset",
        "seed": "prisma db seed",
        "studio": "prisma studio",
        "console": "prisma-repl",
        "format": "prettier --write .",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "Kade Angell, Taylor Rowser, Jared Smith",
    "license": "ISC",
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "prisma": "^5.13.0",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@prisma/client": "^5.13.0",
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-handlebars": "^7.1.2",
        "jsonwebtoken": "^9.0.2",
        "openai": "^4.38.2",
        "prisma-repl": "^1.4.0",
        "socket.io": "^4.7.5"
    },
    "prisma": {
        "schema": "prisma/schema.prisma",
        "seed": "ts-node ./prisma/seeds.ts"
    }
}
